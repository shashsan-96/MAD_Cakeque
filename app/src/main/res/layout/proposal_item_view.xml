
<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"

    android:layout_height="270dp"
    android:background="@color/white"
    android:orientation="vertical">





    <TextView
        android:id="@+id/edittext_User1"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_marginLeft="10dp"
        android:text="userId"
        android:textColor="@color/dogeblu"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/edittext_User"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-35dp"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/text_Name"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="OrderId"
        android:textColor="@color/dogeblu"
        android:textColorHighlight="@color/dogeblu"
        android:textColorHint="@color/dogeblu"
        android:textColorLink="@color/dogeblu"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/edittext_Order"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-35dp"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/text_1"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="Proposition"
        android:textColor="@color/dogeblu"
        android:textColorHighlight="@color/dogeblu"
        android:textColorHint="@color/dogeblu"
        android:textColorLink="@color/dogeblu"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/edittext_Proposition"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-35dp"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/text_2"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="cake cost"
        android:textColor="@color/dogeblu"
        android:textColorHighlight="@color/dogeblu"
        android:textColorHint="@color/dogeblu"
        android:textColorLink="@color/dogeblu"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/edittext_Cakecost"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-35dp"
        android:textColor="@color/black"
        android:textSize="18sp" />


    <TextView
        android:id="@+id/text_3"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="Delivercost"
        android:textColor="@color/dogeblu"
        android:textColorHighlight="@color/dogeblu"
        android:textColorHint="@color/dogeblu"
        android:textColorLink="@color/dogeblu"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/edittext_Delivercost"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-35dp"
        android:textColor="@color/black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/text_4"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:text="Contacts"
        android:textColor="@color/dogeblu"
        android:textColorHighlight="@color/dogeblu"
        android:textColorHint="@color/dogeblu"
        android:textColorLink="@color/dogeblu"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/edittext_Contacts"
        android:layout_width="220dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-35dp"
        android:textColor="@color/black"
        android:textSize="18sp" />


    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_margin="5dp"
        android:background="@color/dogeblu" />
