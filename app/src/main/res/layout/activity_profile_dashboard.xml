<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/chef"
    tools:context=".ProfileDashboard">


    <TextView
        android:id="@+id/textView28"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@drawable/headershape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btncen"
        android:layout_width="339dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:text="@string/btnCreate"
        android:textSize="20sp"
        app:backgroundTint="@color/dogeblu1"
        app:cornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnpro"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnpro"
        android:layout_width="339dp"
        android:layout_height="65dp"
        android:layout_marginTop="8dp"
        android:text="@string/btnView"
        android:textSize="20sp"
        app:backgroundTint="@color/dogeblu1"
        app:cornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.681" />


    <TextView
        android:id="@+id/txt1"
        android:layout_width="245dp"
        android:layout_height="99dp"
        android:fontFamily="@font/avenger"
        android:text="@string/txtprofile"
        android:textAlignment="center"
        android:textSize="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.025" />

    <Button
        android:id="@+id/btnCake"
        android:layout_width="82dp"
        android:layout_height="80dp"
        android:background="@drawable/cake_foreground"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnup"
        android:layout_width="339dp"
        android:layout_height="65dp"
        android:text="@string/btnModify"
        android:textSize="20sp"
        app:backgroundTint="@color/dogeblu1"
        app:cornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.918" />




</androidx.constraintlayout.widget.ConstraintLayout>